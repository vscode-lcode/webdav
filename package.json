{
  "name": "webdav",
  "displayName": "lcode webdav editor",
  "description": "webdav",
  "publisher": "lcode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onFileSystem:webdav",
    "onCommand:lcode.webdav.open"
  ],
  "contributes": {
    "commands": [
      {
        "command": "lcode.webdav.open",
        "title": "Open Webdav URI ...",
        "category": "Remote Workspace"
      }
    ]
  },
  "main": "./lib/extension.js",
  "browser": "./dist/web-bundle.js",
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js",
    "build:web": "webpack"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.68.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.16.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "webdav": "^4.10.0"
  }
}
